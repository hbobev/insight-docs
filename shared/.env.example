# Shared Library Environment Configuration
# This file contains environment variables used by the shared library components

# Application Settings (used by shared config modules)
# Valid values: development, testing, or production
APP_ENV=development
DEBUG=true
# Valid log levels: debug, info, warning, error, critical
LOG_LEVEL=info 

# Authentication & Security
JWT_SECRET_KEY=changeme_use_strong_random_string
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60
# Set to true in production
ENABLE_AUTH=false

# Monitoring
ENABLE_METRICS=true
